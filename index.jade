        .modal-window
        .overlay__close
            p +
        .overlay__bg
            .content
    .view.viewOne#anthony
        .viewOne__animate
            .viewOne__head
                p.viewOne__greet Hi, I'm
                p.viewOne__name Anthony
            .viewOne__specs
                p CODER  /  DESIGNER  /  CREATOR

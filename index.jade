body
    .loading
        .loading__cube
            .cube1
            .cube2
    .header
        .menu
            p.hvr-shutter-out-vertical(href='#anthony') ANTHONY
            p.hvr-shutter-out-vertical(href='#works') WORK
            p.hvr-shutter-out-vertical(href='#contact') CONTACT
